# Skyclaw gateway local config
PORT=3000
DS_PORT=4437
JWT_SECRET=skyclaw-dev-secret
# SKYCLAW_ROOT=~/skyclaw

# Public URL your sprite can reach (required for sprite wake mode)
# Example (local): GATEWAY_URL=https://<your-ngrok-or-cloudflare-url>
GATEWAY_URL=http://localhost:3000

# Sprites integration (optional)
# SPRITES_TOKEN=
SPRITES_API_BASE_URL=https://api.sprites.dev
SPRITE_NAME_PREFIX=skyclaw-
SPRITE_SERVICE_NAME=handler
SPRITE_HANDLER_COMMAND=bunx @skyclaw/handler start
SPRITE_SERVICE_START_DURATION=2s
# SPRITE_FORWARD_ENV=ANTHROPIC_API_KEY,OPENAI_API_KEY,OPENROUTER_API_KEY,SKYCLAW_AGENT_MODEL

# Handler runtime keys (used when running handler directly)
# ANTHROPIC_API_KEY=
# OPENAI_API_KEY=
# OPENROUTER_API_KEY=
# Optional explicit model pattern for oh-my-pi, e.g.
# SKYCLAW_AGENT_MODEL=openrouter/google/gemini-3-flash-preview
